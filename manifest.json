{
  "name": "build Note Generator",
  "description": "build Note Generator",
  "version": "1.0.6",
  "browser_action": {
      "name": "open popup",
      "default_icon":"cloud_16.png",
      "default_title": "build Note Generator",
      "default_popup": "popup.html"
  },
    "background": {
        "persistent": false,
        "scripts": ["builder.js"]
    },
    "content_scripts": [{
        "matches": [ "*://*/*" ],
        "js":      ["jquery.min.js"],
        "run_at": "document_start"
    }],
  "permissions": [
    "tabs","http://*/","https://*/","file:///*", "contextMenus"
  ],
  "icons": {
    "16": "cloud_16.png"
  },
  "manifest_version": 2
}